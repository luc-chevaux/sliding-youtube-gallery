<?xml version="1.0" encoding="UTF-8"?>
<headerInjection>
	<libraries>
		<!-- jquery -->
		<library name="jquery">
			<resource>
				<name>jquery</name>
				<type>js</type>
				<thirdparty>yes</thirdparty>
			</resource>
		</library>
		
		<!-- syg client library -->
		<library name="syg-client" version="1.4.0">
			<resource>
				<name>syg-client-library</name>
				<relUrl>js/core/lib/syg.lib.client.min.js.php</relUrl>
				<type>js</type>
				<version>1.4.0</version>
				<indexed>no</indexed>
				<thirdparty>no</thirdparty>
				<dependencies>
					<dependency>jquery</dependency>
				</dependencies>
			</resource>
		
			<!-- gallery style css -->
			<resource>
				<name>syg-css-style-gallery</name>
				<relUrl>css/SlidingYoutubeGallery.css.php?id=</relUrl>
				<type>css</type>
				<version>1.4.0</version>
				<indexed>yes</indexed>
				<thirdparty>no</thirdparty>
			</resource>
		</library>
		
